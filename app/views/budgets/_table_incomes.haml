%tbody.summary-header.progress
  -@filter.budgets_income.each do |budget|
    %tr
      %td
        =total_category(budget)
      %td.budget
        =progress_bar "success", income_percentage(budget) do
          %p
            =budget.name
        =progress_bar "danger", 100 - income_percentage(budget) do
          %p
            Missing
      %td
        =budget_budgeted(budget)
      %td
        =budget_available(budget)
      %td
        =budget_balance(budget)

